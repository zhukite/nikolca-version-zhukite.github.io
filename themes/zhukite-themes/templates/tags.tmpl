## -*- coding: utf-8 -*-
<%inherit file="base.tmpl"/>
<%block name="content">
	<div class="article_box">
        <div class="article_title">
            <h3>${title}</h3>
        </div>
        <div class="entry" >  
		    <ul class="unstyled bricks">
		    % for text, link in items:
		        <li><a class="reference" href="${link}">${text}</a></li>
		    % endfor    
		    </ul> 
        </div>
    </div>
</%block>
